@model IEnumerable<Web_API_Generator.Models.TableViewModels>

@{
    ViewBag.Title = "Models_Mobile";
}

    <h2>Reponses_Mobile</h2>

<p>
    @Html.ActionLink("Create New", "Add_Models_Mobile", new { id = ViewBag.ProjectID })
</p>
<table class="table">
